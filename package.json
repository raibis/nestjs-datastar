  "description": "A NestJS module for integrating Datastar into your applications.",
  "author": "audrius.raibis@gmail.com",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
