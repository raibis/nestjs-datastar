    "eslint": "9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "globals": "16.3.0",
    "prettier": "3.6.2",
    "pug": "^3.0.3",
    "rimraf": "6.0.1",
    "typescript": "5.9.2",
    "typescript-eslint": "8.43.0"
